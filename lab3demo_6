#include <stdio.h>
int main(){
	int score;
	scanf("%d",&score);
  //bonus2
	if (score <= 100 && score >= 0){
		if (score >= 68){
			if (score >= 85)
				printf("A");
			else if (score >= 75)
				printf("B");
			else { //bonus1
				int scop = 75 - score;
				printf("C %d score=>B",scop);
			}
		}
		else {
			if (score >= 55)
				printf("D");
			else 
				printf("F");
		}
	}
	else 
		printf("error score");
return 0;
}
